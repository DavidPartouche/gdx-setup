<table toTitleTable="true" growX="true">
    <table background="window-bg" onCreate="initTitleTable" growX="true" height="20" padBottom="-20" row="true"
           colspan="3" touchable="enabled"/>
    <image style="libgdx" fill="false" onClick="showSite" expandX="true" padLeft="60" padTop="26"/>
    <imageButton style="close-window" image="icon-minus" onChange="minimize" toTitleTable="true" fill="false" align="topRight"/>
    <imageButton style="close-window" onChange="app:exit" toTitleTable="true" fill="false" expandX="false"
                 padLeft="3" align="topRight"/>
</table>
